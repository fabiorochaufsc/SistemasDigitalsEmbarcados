

PROJECT = main



CC      = gcc
CPP     = g++
CCLOCAL = gcc

CC_FLAGS = -D_USA_LINUX_  -Wall  -g 

MEUPROJETO = main.o  memoria.o




$(PROJECT).exe: $(MEUPROJETO) 
	gcc main.o memoria.o -o $(PROJECT).exe
	






main.o:	main.c
	# *************************** main *****************************
	$(CC) $(CC_FLAGS)  -std=gnu99 main.c -c
	# **************************************************************

memoria.o:  memoria.h i2c_software.o memoria.c
	$(CC) $(CC_FLAGS)  -std=gnu99 memoria.c -c


clean:
	rm -f  src/*.o 
